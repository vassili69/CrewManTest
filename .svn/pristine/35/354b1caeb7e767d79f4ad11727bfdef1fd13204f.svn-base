.geral { /* 100% de altura e largura geral fixed=janela browser*/
    background: #fff;
    margin: auto;
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    display: block;
}

.div_geral_esquerda { /* divisão horizontal esquerda com 25% de 100% do geral */
    width: 25%;
    height: 100%;
    background: #000;
    float: left;
    border-right: solid 10px #FFF;
    box-sizing: border-box;
}

.div_geral_direita { /* divisão horizontal direita-centro com 75% de 100% do geral */
    position: relative;
    width: 75%;
    height: 100%;
    background: #FFF;
    float: left;
    overflow: auto; /* garante scroll automático se necessário */
}


