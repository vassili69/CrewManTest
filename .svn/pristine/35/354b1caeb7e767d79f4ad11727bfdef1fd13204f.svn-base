/*
===========================================
Div popup imagem
===========================================
*/

#cover_imagem { /* capa sobre tudo */
    position: fixed;
    top: 0;
    left: 0;
    background: #333333;
    //z-index:5;
    z-index: 5;
    width: 100%;
    height: 100%;
    display: none;
}


#popup_imagem { /* Conteudo do div 'popup' */
    border: 1px #FF6600 solid;
    margin: auto;
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    width: 400px;
    height: 150px;
    z-index: 10;
    display: none;
    background-color: #FFF;
    padding: 30px;
    box-shadow: 0px 3px 3px #666666;
    overflow: hidden;
}

    #popup_imagem:target {
        display: block;
    }

        #popup_imagem:target + #cover_imagem {
            display: block;
            position: fixed;
            top: 0;
            left: 0;
            /* opacidade */
            -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=50)"; /* IE 8 */
            filter: alpha(opacity=50); /* IE 5-7 */
            -moz-opacity: 0.5; /* Netscape */
            -khtml-opacity: 0.5; /* Safari 1.x */
            opacity: 0.5; /* Good browsers */
        }

.popup_titulo_cabecalho {
    font: 20px arial, sans-serif;
    color: #FF6600;
    padding-bottom: 20px;
}

.popup_input_upload {
    width: 100%;
    font: 14px arial, sans-serif;
    color: #999999;
}

.popup_botoes {
    width: 140px;
    float: left;
}
